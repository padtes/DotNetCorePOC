		"data_table_name":"filedetails",
		"data_table_json_col":"data",
		"unique_column":"pran_id",
		"courier_col":"courier_id"
	}
	,"database":
	[
		{
		"row_type" : "fh",
		"H004_batch_id": "batch_id"
		}
	  ,{
		  "row_type" : "pd",
		  "P006_courier_id":"courier_id",
		  "P009_acknowledgement_number": "ack_number",
		  "P008_pran" : "pran_id",
		  "P048_photograph" : "photograph",
		  "P049_signature" :"signature"
	  }	
	]
	,"json_skip":[
		{
			"row_type" : "fh",
			"cols":["H009_record_level_hash"]
		}
		,{
		  "row_type" : "pd",
		  "cols":["P009_acknowledgement_number","P008_pran","P048_photograph","P049_signature"]
		}
	]
	,"save_as_file":
	[
	  {
		  "row_type" : "pd",
		  "columns": 
		  [
			{"colName":"P048_photograph", "dir":"Photo","sub_dir":"Photo_","file_name" : "{{courier_seq}}_{{unique_column}}_photo.jpg"},
